#include "main.h"
#include <stdio.h>
#include <stdlib.h>

/**
 * main - A function that prints its name  on the command line
 *
 * @argc: An integer arguement
 * @argv: A pointer to an array of strings
 *
 * Return: Returns 0
 */

int main(int argc, char *argv[] __attribute__((unused)))
{
	int i = argc - 1;

	if (i == 2)
	{
		int j = *argv[1];
		int k = *argv[2];

		printf("%d\n", j * k);
		return (0);
	}
	else
	{
		printf("Error\n");
		return (1);
	}
}
